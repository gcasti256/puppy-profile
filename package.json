{
  "name": "bootstrap-v5",
  "version": "1.0.0",
  "description": "Learning bootstrap v5 with sass",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel compile:sass browsersync",
    "compile:sass": "sass --watch scss:assets/css",
    "browsersync": "browser-sync start --server --files \"**/*\" && sass --watch scss:assets/css"
  },
  "keywords": [
    "Bootstrap",
    "bootstrap-v5",
    "sass",
    "learn-bootstrap",
    "learn-sass"
  ],
  "author": "Patrick Muriungi",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap": "^5.0.0"
  }
}
